# Environment Selector
# Set to 'true' to use production KHSS endpoints, 'false' for test environment
USE_PRODUCTION=false

# ===================================
# KHSS TEST ENVIRONMENT
# ===================================
KHSS_TEST_BASE_URL=test.salesforce.com
KHSS_TEST_INSTANCE_URL=your-test-instance.salesforce.com
KHSS_TEST_CONSUMER_KEY=your_test_consumer_key_here
KHSS_TEST_CONSUMER_SECRET=your_test_consumer_secret_here
KHSS_TEST_USERNAME=your_test_username@domain.com
KHSS_TEST_PASSWORD=your_test_password_and_security_token
KHSS_TEST_PARTNER_ID=DL0923
KHSS_TEST_ACTIVITY_ID=1

# ===================================
# KHSS PRODUCTION ENVIRONMENT
# ===================================
KHSS_PROD_BASE_URL=login.salesforce.com
KHSS_PROD_INSTANCE_URL=your-prod-instance.salesforce.com
KHSS_PROD_CONSUMER_KEY=your_prod_consumer_key_here
KHSS_PROD_CONSUMER_SECRET=your_prod_consumer_secret_here
KHSS_PROD_USERNAME=your_prod_username@domain.com
KHSS_PROD_PASSWORD=your_prod_password_and_security_token
KHSS_PROD_PARTNER_ID=DL0923
KHSS_PROD_ACTIVITY_ID=1

# ===================================
# SUGGESTIC API (existing)
# ===================================
GRAPHQL_ENDPOINT=https://production.suggestic.com/graphql
GRAPHQL_API_TOKEN=your_suggestic_api_token_here
SUGGESTIC_USER_ID=your_suggestic_user_id_here
